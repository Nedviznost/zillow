    "@reach/menu-button": "^0.16.2",
    "@reach/skip-nav": "^0.16.0",
    "@react-icons/all-files": "^4.1.0",
    "@reduxjs/toolkit": "^1.7.2",
    "@sentry/nextjs": "^6.18.2",
